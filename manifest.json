{
  "manifest_version": 2,

  "name": "Typer",
  "description": "This extension of Dymas",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://vk.com/*"],
      "js": ["jquery.js", "myscript.js"]
    }
  ],
  "browser_action": {
   "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  }

}